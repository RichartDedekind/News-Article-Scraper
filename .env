# Proxy settings
# Uncomment and set these values to use a proxy

# HTTP proxy (format: http://username:password@host:port)
# HTTP_PROXY=http://user:pass@proxy.example.com:8080

# HTTPS proxy (format: http://username:password@host:port)
# HTTPS_PROXY=http://user:pass@proxy.example.com:8080

# SOCKS proxy (format: socks5://username:password@host:port)
# SOCKS_PROXY=socks5://user:pass@proxy.example.com:1080

# SmartProxy settings
SMARTPROXY_USER=user
SMARTPROXY_PASS=pass
SMARTPROXY_HOST=gate.smartproxy.com
SMARTPROXY_PORT=7000

# Proxy list settings
# Path to a file containing a list of proxies (one per line)
PROXY_LIST_FILE=proxies.txt
# Whether to rotate through proxies in the list
USE_PROXY_LIST=false

# Proxy type: 'http', 'https', 'socks5', 'smartproxy', 'proxy_list', or 'none'
PROXY_TYPE=none

# Delay between requests in milliseconds (to avoid rate limiting)
REQUEST_DELAY=2000

# Whether to rotate user agents for each request
USE_RANDOM_USER_AGENT=true

# Whether to respect robots.txt
RESPECT_ROBOTS_TXT=true
